    print(f"Eigenvector: {pca.components_[i]}")
    print(f"Eigenvalue: {pca.explained_variance_[i]}")
    print(f"Variance: {pca.explained_variance_[i] / np.sum(pca.explained_variance_)}")
    print("\n")

